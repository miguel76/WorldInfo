@prefix :        <http://www.cattid.uniroma1.it/2012/pampers#> .
@prefix svg:     <http://uispin.org/svg#> .
@prefix uix:     <http://www.swows.org/uix#> .
@prefix tuio:    <http://www.swows.org/tuio#> .
@prefix xml:    <http://www.swows.org/xml#> .
@prefix sp:      <http://spinrdf.org/sp#> .
@prefix df:   <http://www.swows.org/dataflow#> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .
@prefix vp:   <http://www.swows.org/viewport#> .
@prefix swi:   <http://www.swows.org/instance#> .
@prefix rdfSyntax:   <http://www.swows.org/syntaxes/RDF/> .
@prefix syntax:   <http://www.swows.org/syntaxes/> .
@prefix query: <http://jena.hpl.hp.com/2003/07/query/> .

swi:OutputDataset
  a df:InlineDataset ;
    df:input
      [ a df:UnionGraph ;
        df:input _:svgVar ; 
        df:input _:svgDoc 
      ] .

_:svgVar
  a df:ConstructGraph ;
  df:input
    [ a df:InlineDataset ;
      df:input _:mainInfo ;
      df:namedInput 
        [ a df:NamedGraph ;
          df:id :descendants ;
          df:input _:descendants ] ;
      df:namedInput 
        [ a df:NamedGraph ;
          df:id :selectedOption ;
          df:input _:selectedOption ] ;
      df:namedInput 
        [ a df:NamedGraph ;
          df:id :config ;
          df:input _:config ] ] ;
  df:config
    [ a df:IncludedGraph ;
      df:url <svg.sparql> ;
      df:syntax query:SPARQL_11 ].

_:descendants
  a df:ConstructGraph ;
  df:input
    [ a df:InlineDataset ;
      df:input :svgDoc ] ;
  df:config
    [ a df:IncludedGraph ;
      df:url <children2descendants.sparql> ;
      df:syntax query:SPARQL_11 ] .

_:svgDoc
  a df:IncludedGraph ;
  df:url <../svg/BlankMapWithRadioBox.svg> ;
  df:syntax syntax:XML .

_:mainInfo
  a df:SelectGraph ;
  df:input swi:InputDataset .

_:selectedOption
  a df:SelectGraph ;
  df:id :selectedOption ;
  df:input swi:InputDataset .

_:config
  a df:SelectGraph ;
  df:id :config ;
  df:input swi:InputDataset .



